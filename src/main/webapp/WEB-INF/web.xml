<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>srvCategoria</servlet-name>
        <servlet-class>com.agromaquinariasosorio.controlador.srvCategoria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>srvProductos</servlet-name>
        <servlet-class>com.agromaquinariasosorio.controlador.srvProductos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>srvPedido</servlet-name>
        <servlet-class>com.agromaquinariasosorio.controlador.srvPedido</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>srvUsuario</servlet-name>
        <servlet-class>com.agromaquinariasosorio.controlador.srvUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>srvCategoria</servlet-name>
        <url-pattern>/srvCategoria</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>srvProductos</servlet-name>
        <url-pattern>/srvProductos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>srvPedido</servlet-name>
        <url-pattern>/srvPedido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>srvUsuario</servlet-name>
        <url-pattern>/srvUsuario</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
